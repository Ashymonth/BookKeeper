<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="BookKeeperDb" connectionString="data source=(localdb)\MSSQLLocalDB;Initial Catalog=BookKeeper;Integrated Security=True;" 
             providerName="System.Data.SqlClient" />
    </connectionStrings>

  <appSettings>
    <add key="ConnectionName" value="BookKeeperDb" />
    <add key="BackupFolder" value="Backups"/>
    <add key="BackupFileTemplate" value="{0}.bak"/>
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  
</configuration>